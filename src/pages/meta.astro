---
import DEFAULTS, * as SETTINGS from '../SETTINGS.mjs';
const {env} = process;
---

<!DOCTYPE html>
<html>
  <body>
    <table border="1">
      <tr>
        <th></th>
        <th>DEFAULT</th>
        <th>ENV</th>
        <th>FINAL</th>
      </tr>
      {Object.keys(DEFAULTS).map((key) => (
        <tr>
          <td>{key}</td>
          <td>{DEFAULTS[key]}</td>
          <td>{env[key]}</td>
          <td>{SETTINGS[key]}</td>
        </tr>
      ))}
    </table>
  </body>
</html>

---
import {
        SITE_FAVICON_TYPE,
        SITE_BASE_PATH,
        SITE_BLOG_PAGE_SIZE,
        SITE_LIB_PATH,
        SITE_CANONICAL_URL,
        SITE_DESCRIPTION,
        SITE_KEYWORDS,
        TAG_MANAGER_ID } from '../SETTINGS.mjs';
---
<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <meta name="" content={SITE_FAVICON_TYPE}>
  <meta name="" content={SITE_BASE_PATH}>
  <meta name="" content={SITE_BLOG_PAGE_SIZE}>
  <meta name="" content={SITE_LIB_PATH}>
  <meta name="" content={SITE_CANONICAL_URL}>
  <meta name="" content={SITE_DESCRIPTION}>
  <meta name="" content={SITE_KEYWORDS.join(",")}>
  <meta name="" content={TAG_MANAGER_ID}>
  </head>
</html>
